{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "ie >= 9" ]
      }
    }],
    "stage-2"
  ],
  "plugins": ["transform-runtime", "syntax-dynamic-import", "transform-vue-jsx" ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["istanbul"]
    }
  }
}
